<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <artifactId>odlparent</artifactId>
    <groupId>io.frinx.uniconfig.common</groupId>
    <relativePath>../../../../commons/parents/odlparent</relativePath>
    <version>7.0.3-SNAPSHOT</version>
  </parent>

  <groupId>io.frinx.uniconfig.cli</groupId>
  <artifactId>cli-units-saos-6</artifactId>
  <version>7.0.3-SNAPSHOT</version>
  <packaging>pom</packaging>

  <modules>
    <module>saos-6-init</module>
    <module>saos-6-configuration-metadata</module>
    <module>saos-6-it</module>
    <module>saos-6-network-instance</module>
    <module>saos-8-network-instance</module>
    <module>saos-6-interface</module>
    <module>saos-8-interface</module>
    <module>saos-6-qos</module>
    <module>saos-6-broadcast-containment</module>
    <module>saos-6-l2-cft</module>
    <module>saos-6-acl</module>
    <module>saos-6-ring</module>
    <module>saos-8-relay-agent</module>
    <module>saos-6-platform</module>
    <module>saos-8-platform</module>
    <module>saos-6-relay-agent</module>
    <module>saos-6-system</module>
    <module>saos-8-system</module>
    <module>saos-6-aaa</module>
  </modules>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-deploy-plugin</artifactId>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-install-plugin</artifactId>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
